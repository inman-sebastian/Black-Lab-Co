    {
        "name":"Shop Typography",
        "settings":[
            {
                "type":"header",
                "content":"Font Family",
                "info":"Font family for body"
            },
            {
                "type":"radio",
                "id":"vela_fontfamily_style",
                "label":"Fonts style",
                "options":[
                    {
                        "value":"google_fonts",
                        "label":"Google font"
                    },
                    {
                        "value":"upload_fonts",
                        "label":"Custom font"
                    }
                ],
                "info":"[Where do I find Google Fonts?](http:\/\/www.google.com\/fonts)",
                "default":"google_fonts"
            },
            {
                "type":"text",
                "id":"vela_fontfamily_link",
                "label":"Google font URL"
            },
            {
                "type":"text",
                "id":"vela_fontfamily",
                "label":"Font name",
                "info":"Use for google font and custom font",
                "default":"Arial"
            },
            {
                "type":"header",
                "content":"Font Family Secondary",
                "info":"Font family for menu"
            },
            {
                "type":"radio",
                "id":"vela_fontfamily_secondary_style",
                "label":"Fonts style",
                "options":[
                    {
                        "value":"google_fonts",
                        "label":"Google font"
                    },
                    {
                        "value":"upload_fonts",
                        "label":"Custom font"
                    }
                ],
                "info":"[Where do I find Google Fonts?](http:\/\/www.google.com\/fonts)",
                "default":"google_fonts"
            },
            {
                "type":"text",
                "id":"vela_fontfamily2_link",
                "label":"Google font URL"
            },
            {
                "type":"text",
                "id":"vela_fontfamily_secondary",
                "label":"Fonts name",
                "info":"Use for google font and custom font",
                "default":"Arial"
            },
            {
                "type":"header",
                "content":"Font Size"
            },
            {
                "type":"range",
                "id":"vela_fontsize",
                "min":10,
                "max":24,
                "step":1,
                "unit":"px",
                "label":"Font size",
                "default":14,
                "info":"Font body size"
            },
            {
                "type":"range",
                "id":"vela_headingsize",
                "min":10,
                "max":100,
                "step":1,
                "unit":"px",
                "label":"Font heading size",
                "default":14,
                "info":"Font heading size"
            },
            {
                "type":"range",
                "id":"velamenu_fontsize",
                "min":10,
                "max":24,
                "step":1,
                "unit":"px",
                "label":"Menu font size",
                "default":14,
                "info":"Font menu"
            },
            {
                "type":"range",
                "id":"velamenu_dropdown_fontsize",
                "min":10,
                "max":24,
                "step":1,
                "unit":"px",
                "label":"Dropdown menu font size",
                "default":14,
                "info":"Font dropdown menu size "
            }
        ]
    },
    {
        "name":"Shop Features",
        "settings":[
            {
                "type":"header",
                "content":"Theme Settings"
            },
            {
                "type":"image_picker",
                "id":"favicon",
                "label":"Favicon image",
                "info":"Will be scaled down to 32 x 32px"
            },
            {
                "type":"text",
                "id":"social_twitter_link",
                "label":"Twitter Link",
                "info":"https:\/\/twitter.com\/shopify"
            },
            {
                "type":"header",
                "content":"Product Reviews"
            },
            {
                "type":"checkbox",
                "id":"product_reviews_enable",
                "label":"Enable",
                "info":"[View Product Review settings](\/admin\/apps\/product-reviews\/settings)"
            },
            {
                "type":"header",
                "content":"Product Box Item"
            },
            {
                "type":"checkbox",
                "id":"productgrid_swatch",
                "label":"Show color variant"
            },
            {
                "type":"checkbox",
                "id":"productgrid_type",
                "label":"Show product type"
            },
            {
                "type":"checkbox",
                "id":"productgrid_review",
                "label":"Show review"
            },
            {
                "type":"checkbox",
                "id":"productgrid_shortdesc",
                "label":"Show short description"
            },
            {
                "type":"checkbox",
                "id":"productgrid_sale",
                "label":"Show label sale"
            },
            {
                "type":"header",
                "content":"Loading Site"
            },
            {
                "type":"checkbox",
                "id":"loading_enable",
                "label":"Enable"
            },
            {
                "type":"header",
                "content":"Float header"
            },
            {
                "type":"checkbox",
                "id":"header_float_enable",
                "label":"Enable"
            },
            {
                "type":"header",
                "content":"Go To Top"
            },
            {
                "type":"checkbox",
                "id":"gotop_enable",
                "label":"Enable"
            },
            {
                "type":"header",
                "content":"Product Variants"
            },
            {
                "type":"checkbox",
                "id":"product_swatch_unvailable",
                "label":"Show unvailable",
                "info":"Show unavailable product variants."
            },
            {
                "type":"checkbox",
                "id":"product_swatch",
                "label":"Enable",
                "info":"Use swatch variants for product page and quickview."
            },
            {
                "type":"header",
                "content":"Second Product Image"
            },
            {
                "type":"checkbox",
                "id":"product_second_image",
                "label":"Enable",
                "info":"Show second product image when hover on product item."
            },
            {
                "type":"header",
                "content":"Filter Collection Site"
            },
            {
                "type":"checkbox",
                "id":"sidebar_filtercustag_multi",
                "label":"Enable multiple choices",
                "info":"User can select multiple (Size, price, color) or only one (Size, price, color)"
            },
            {
                "type":"header",
                "content":"Product Quickview",
                "info":"Quickview will disable on Mobile, Tablet"
            },
            {
                "type":"checkbox",
                "id":"quickview_enable",
                "label":"Enable"
            },
            {
                "type":"checkbox",
                "id":"quickview_availability",
                "label":"Show availability"
            },
            {
                "type":"checkbox",
                "id":"quickview_vendor",
                "label":"Show vendor"
            },
            {
                "type":"checkbox",
                "id":"quickview_protype",
                "label":"Show product type"
            },
            {
                "type":"checkbox",
                "id":"quickview_prosku",
                "label":"Show product SKU"
            },
            {
                "type":"header",
                "content":"Ajax Cart"
            },
            {
                "type":"select",
                "id":"ajax_cart_method",
                "label":"Cart type",
                "options":[
                    {
                        "value":"modal",
                        "label":"Modal"
                    },
                    {
                        "value":"drawer",
                        "label":"Drawer"
                    },
                    {
                        "value":"page",
                        "label":"Page"
                    }
                ]
            },
            {
                "type":"checkbox",
                "id":"cart_notes_enable",
                "label":"Show cart notes"
            },
            {
                "type":"header",
                "content":"Google map"
            },
            {
                "type":"checkbox",
                "id":"googlemap_enable",
                "label":"Enable",
                "default":true
            },
            {
                "type":"text",
                "id":"googlemap_key",
                "label":"Input key for googlemap",
                "default":"AIzaSyDqstP8RWMwkuJYsaWQ29dZFim3506MteA",
                "info":"you can register key free [at here](https:\/\/cloud.google.com\/maps-platform\/)"
            }
        ]
    },
    {
        "name":"Shop Newsletter Popup",
        "settings":[
            {
                "type":"checkbox",
                "id":"vela_newslettermodal_enable",
                "label":"Enable"
            },
            {
                "type":"text",
